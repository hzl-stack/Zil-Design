{
  "name": "Zil-Design",
  "version": "0.0.1",
  "private": true,
  "description": "React library by hzl",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hzl-stack/Zil-Design.git",
    "branch": "master",
    "platform": "github"
  },
  "license": "MIT",
  "sideEffects": [
    "*.less"
  ],
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "workspaces": [
    "packages/**/*"
  ],
  "scripts": {
    "build": "father build",
    "build:watch": "father dev",
    "clean": "rimraf node_modules yarn.lock && lerna clean",
    "clean:dist": "lerna run clean --no-private",
    "compile:all": "yarn clean:dist && lerna run build --no-private",
    "dev": "dumi dev",
    "docs:build": "dumi build",
    "docs:preview": "dumi preview",
    "doctor": "father doctor",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:css": "stylelint \"{src,test}/**/*.{css,less}\"",
    "lint:es": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install && dumi setup",
    "prepublishOnly": "father doctor && npm run build",
    "pub": "lerna publish from-git",
    "start": "npm run dev",
    "version:major": "lerna version major --no-private",
    "version:minor": "lerna version minor --no-private",
    "version:patch": "lerna version patch --no-private"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@umijs/lint": "^4.0.0",
    "dumi": "^2.3.0",
    "eslint": "^8.23.0",
    "father": "^4.1.0",
    "husky": "^8.0.1",
    "lerna": "^8.1.8",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-packagejson": "^2.2.18",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "stylelint": "^14.9.1"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "authors": [
    "HuZili <1571398377@qq.com>"
  ]
}
